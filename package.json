{
  "name": "training-app",
  "version": "1.0.0",
  "description": "Personal workout tracker with PDF upload and progress tracking",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
